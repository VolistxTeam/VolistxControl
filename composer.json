{
  "name": "volistx/volistx-control",
  "description": "Volistx Framework Kernel Control Library",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "illuminate/cache": "^8.0|^9.0|^10.0",
    "illuminate/console": "^8.0|^9.0|^10.0",
    "illuminate/support": "^8.0|^9.0|^10.0",
    "guzzlehttp/guzzle": "^7.7",
    "volistx/validation": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0|^10.0",
    "mockery/mockery": "^1.3",
    "laravel/pint": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "Volistx\\Control\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Volistx\\Control\\VolistxControlProvider"
      ],
      "aliases": {
        "VolistxControl": "Volistx\\Control\\Facades\\VolistxControlFacade"
      }
    }
  }
}
